    <!-- Dialog title -->
    <string name="niu_actions_confirmation_title">Traducir o escuchar texto en pantalla</string>
    <!-- Dialog content -->
    <string name="niu_actions_confirmation_text">Puede compartir información como texto en su pantalla, direcciones web y capturas de pantalla con Google.\n\nPara cambiar qué información comparte, ir a <b>Ajustes &gt; Aplicaciones &gt; Aplicaciones predeterminadas &gt; Asistente digital</b>.</string>
