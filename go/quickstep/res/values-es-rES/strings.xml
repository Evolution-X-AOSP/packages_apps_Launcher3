    <string name="assistant_not_selected_text">Para escuchar o traducir texto en tu pantalla, elige una aplicación de asistente digital en Ajustes</string>
    <!-- Assistant not supported: Dialog title -->
    <string name="assistant_not_supported_title">Cambia tu asistente para usar esta función</string>
