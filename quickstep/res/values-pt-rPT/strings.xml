    <!-- Application name -->
    <!-- Options for recent tasks -->
    <!-- Title for an option to keep an app pinned to the screen until it is unpinned -->
    <string name="recent_task_option_pin">Fixar</string>
    <!-- Title for an option to enter freeform mode for a given app -->
    <string name="recent_task_option_freeform">Forma livre</string>
    <!-- Recents: The empty recents string. [CHAR LIMIT=NONE] -->
    <string name="recents_empty_message">Nenhum item recente</string>
    <!-- Content description for the recent apps's accessibility option that opens its usage settings. [CHAR LIMIT=NONE] -->
    <string name="accessibility_app_usage_settings">Definições de utilização de aplicações</string>
    <!-- Recents: Title of a button that clears the task list, i.e. closes all tasks. [CHAR LIMIT=30] -->
    <string name="recents_clear_all">Limpar tudo</string>
    <!-- Accessibility title for the list of recent apps [CHAR_LIMIT=none] -->
    <string name="accessibility_recent_apps">Apps recentes</string>
    <!-- Accessibility confirmation for task closed -->
    <string name="task_view_closed">Tarefa fechada</string>
    <!-- Accessibility title for an app card in Recents for apps that have time limit set
     [CHAR_LIMIT=none] -->
    <string name="task_contents_description_with_remaining_time"><xliff:g id="TASK_DESCRIPTION">%1$s</xliff:g>, <xliff:g id="REMAINING_TIME">%2$s</xliff:g></string>
    <!-- Text to show total app usage per day if it is less than 1 minute ("&lt;" is the
     escaped form of '<'). [CHAR LIMIT=15] -->
    <string name="shorter_duration_less_than_one_minute">&lt; 1 minuto</string>
    <!-- Annotation shown on an app card in Recents, telling that the app has a usage limit set by
    the user, and a given time is left for it today [CHAR LIMIT=22] -->
    <string name="time_left_for_app">Resta(m) <xliff:g id="TIME">%1$s</xliff:g> hoje.</string>
    <!-- Accessibility title for the row of all-apps containing app predictions. [CHAR LIMIT=50] -->
    <string name="title_app_suggestions">Sugestões de apps</string>
    <!-- Text of the tip when user lands in all apps view for the first time, indicating where the tip toast points to is the predicted apps section. [CHAR_LIMIT=50] -->
    <string name="all_apps_prediction_tip">As suas apps previstas</string>
    <!-- Content description for a close button. [CHAR LIMIT=NONE] -->
    <!-- Hotseat educational strings for users who don't qualify for migration -->
    <string name="hotseat_edu_title_migrate">Receba sugestões de apps na última fila do ecrã principal</string>
    <string name="hotseat_edu_title_migrate_landscape">Receba sugestões de apps na fila dos favoritos do ecrã principal</string>
    <string name="hotseat_edu_message_migrate">Aceda facilmente às suas apps mais utilizadas, diretamente no ecrã principal. As sugestões mudam em função das suas rotinas. As apps na última fila passam para o ecrã principal.</string>
    <string name="hotseat_edu_message_migrate_landscape">Aceda facilmente às suas apps mais utilizadas no ecrã principal. As sugestões mudam em função das suas rotinas. As apps na fila dos favoritos passam para o ecrã principal.</string>
    <!-- Button text to opt in for fully predicted hotseat -->
    <string name="hotseat_edu_accept">Obter sugestões de apps</string>
    <!-- Button text to dismiss opt in for fully predicted hotseat -->
    <string name="hotseat_edu_dismiss">Não, obrigado</string>
    <!-- action shown to toggle predictions after onboarding -->
    <string name="hotseat_prediction_settings">Definições</string>
    <!-- tip shown if user has no items in hotseat to migrate -->
    <string name="hotseat_auto_enrolled">As apps mais utilizadas aparecem aqui e mudam em função das rotinas.</string>
    <!-- tip shown if user declines migration and has no open spots for prediction -->
    <string name="hotseat_tip_no_empty_slots">Arraste as apps para fora da última fila para ver sugestões de apps.</string>
    <!-- tip shown if user declines migration and has some open spots for prediction -->
    <string name="hotseat_tip_gaps_filled">Sugestões de apps adicionadas a um espaço vazio.</string>
    <!-- tip shown when user migrates and predictions are enabled in hotseat -->
    <string name="hotsaet_tip_prediction_enabled">Sugestões de apps ativadas</string>
    <!-- tip shown when hotseat edu is requested while predictions are disabled -->
    <string name="hotsaet_tip_prediction_disabled">As sugestões de apps estão desativadas</string>
    <!-- content description for hotseat items -->
    <string name="hotseat_prediction_content_description">App prevista: <xliff:g id="TITLE">%1$s</xliff:g></string>
    <!-- Feedback shown during interactive parts of Back gesture tutorial for right edge when the gesture is too far from the edge. [CHAR LIMIT=100] -->
    <string name="back_gesture_feedback_swipe_too_far_from_edge">Deslize rapidamente a partir da extremidade mais à direita ou mais à esquerda.</string>
    <!-- Feedback shown during interactive parts of Back gesture tutorial for right edge when the gesture is cancelled. [CHAR LIMIT=100] -->
    <string name="back_gesture_feedback_cancelled">Deslize rapidamente a partir da extremidade esquerda ou direita até ao centro do ecrã e solte.</string>
    <!-- Feedback shown after completing the back gesture step if the user is following the full gesture tutorial flow. [CHAR LIMIT=100] -->
    <string name="back_gesture_feedback_complete_with_overview_follow_up">Aprendeu a deslizar a partir da direita para retroceder. A seguir, saiba como alternar entre apps.</string>
    <!-- Feedback shown after completing the back gesture step if the user started this tutorial individually. [CHAR LIMIT=100] -->
    <string name="back_gesture_feedback_complete_without_follow_up">Concluiu o gesto para retroceder.</string>
    <!-- Feedback shown during interactive parts of Back gesture tutorial when the gesture is within the nav bar region. [CHAR LIMIT=100] -->
    <string name="back_gesture_feedback_swipe_in_nav_bar">Garanta que não desliza rapidamente com o dedo demasiado perto da parte inferior do ecrã.</string>
    <!-- Subtitle shown on the confirmation screen after successful gesture. [CHAR LIMIT=60] -->
    <string name="back_gesture_tutorial_confirm_subtitle">Altere a sensibilidade do gesto para voltar nas Definições.</string>
    <!-- Feedback shown during interactive parts of Home gesture tutorial when the gesture is started too far from the edge. [CHAR LIMIT=100] -->
    <!-- Introduction title for the Back gesture tutorial. [CHAR LIMIT=100] -->
    <string name="back_gesture_intro_title">Deslize rapidamente com o dedo para retroceder</string>
    <!-- Introduction subtitle for the Back gesture tutorial. [CHAR LIMIT=200] -->
    <string name="back_gesture_intro_subtitle">Para voltar ao último ecrã, deslize rapidamente do limite esquerdo ou direito até ao centro do ecrã.</string>
    <!-- Introduction subtitle for the Back gesture tutorial that will be spoken by screen readers. [CHAR LIMIT=200] -->
    <string name="back_gesture_spoken_intro_subtitle">Para voltar ao último ecrã, deslize rapidamente com 2 dedos a partir da extremidade esquerda ou direita até ao centro do ecrã.</string>
    <string name="home_gesture_feedback_swipe_too_far_from_edge">Garanta que desliza rapidamente com o dedo a partir do limite inferior do ecrã.</string>
    <!-- Feedback shown during interactive parts of Home gesture tutorial when the Overview gesture is detected. [CHAR LIMIT=100] -->
    <string name="home_gesture_feedback_overview_detected">Garanta que não faz uma pausa antes de soltar.</string>
    <!-- Feedback shown during interactive parts of Home gesture tutorial when the gesture is horizontal instead of vertical. [CHAR LIMIT=100] -->
    <string name="home_gesture_feedback_wrong_swipe_direction">Garanta que desliza rapidamente com o dedo para cima.</string>
    <!-- Feedback shown after completing the home gesture step if the user is following the full gesture tutorial flow. [CHAR LIMIT=100] -->
    <string name="home_gesture_feedback_complete_with_follow_up">Concluiu o gesto para aceder ao ecrã principal. A seguir, saiba como retroceder.</string>
    <!-- Feedback shown after completing the home gesture step if the user started this tutorial individually. [CHAR LIMIT=100] -->
    <string name="home_gesture_feedback_complete_without_follow_up">Concluiu o gesto para aceder ao ecrã principal.</string>
    <!-- Introduction title for the Home gesture tutorial. [CHAR LIMIT=100] -->
    <string name="home_gesture_intro_title">Deslize rapidamente com o dedo para aceder ao ecrã principal</string>
    <!-- Introduction subtitle for the Home gesture tutorial. [CHAR LIMIT=100] -->
    <string name="home_gesture_intro_subtitle">Deslize rapidamente para cima a partir da parte inferior. Este gesto abre sempre o ecrã principal.</string>
    <!-- Introduction subtitle for the Home gesture tutorial that will be spoken by screen readers. [CHAR LIMIT=100] -->
    <string name="home_gesture_spoken_intro_subtitle">Deslize rapidamente para cima com 2 dedos no fundo do ecrã. Este gesto abre sempre o ecrã principal.</string>
    <!-- Feedback shown during interactive parts of Overview gesture tutorial when the gesture is started too far from the edge. [CHAR LIMIT=100] -->
    <string name="overview_gesture_feedback_swipe_too_far_from_edge">Deslize rapidamente com o dedo a partir do limite inferior do ecrã.</string>
    <!-- Feedback shown during interactive parts of Overview gesture tutorial when the Home gesture is detected. The window refers to the current app's window during the gesture. [CHAR LIMIT=100] -->
    <string name="overview_gesture_feedback_home_detected">Experimente premir a janela durante mais tempo antes de soltar.</string>
    <!-- Feedback shown during interactive parts of Overview gesture tutorial when the gesture is horizontal instead of vertical. [CHAR LIMIT=100] -->
    <string name="overview_gesture_feedback_wrong_swipe_direction">Garanta que desliza rapidamente com o dedo para cima e, em seguida, faz uma pausa.</string>
    <!-- Feedback shown after completing the overview gesture step if the user is following the full gesture tutorial flow. [CHAR LIMIT=100] -->
    <string name="overview_gesture_feedback_complete_with_follow_up">Aprendeu a utilizar gestos. Para desativar os gestos, aceda às Definições.</string>
    <!-- Feedback shown after completing the overview gesture step if the user started this tutorial individually. [CHAR LIMIT=100] -->
    <string name="overview_gesture_feedback_complete_without_follow_up">Concluiu o gesto para alternar entre apps.</string>
    <!-- Introduction title for the Overview gesture tutorial. [CHAR LIMIT=100] -->
    <string name="overview_gesture_intro_title">Deslize rapidamente com o dedo para alternar entre apps</string>
    <!-- Introduction subtitle for the Overview gesture tutorial. [CHAR LIMIT=100] -->
    <string name="overview_gesture_intro_subtitle">Para alternar entre apps, deslize para cima sem soltar a partir da parte inferior do ecrã e solte.</string>
    <!-- Introduction subtitle for the Overview gesture tutorial that will be spoken by screen readers. [CHAR LIMIT=100] -->
    <string name="overview_gesture_spoken_intro_subtitle">Para mudar de app, deslize rapidamente para cima com 2 dedos sem soltar no fundo do ecrã e solte.</string>
    <!-- Title shown during interactive part of Assistant gesture tutorial. [CHAR LIMIT=30] -->
    <!-- Subtitle shown during interactive parts of Assistant gesture tutorial. [CHAR LIMIT=60] -->
    <!-- Feedback shown during interactive parts of Assistant gesture tutorial when the gesture is started too far from the corner. [CHAR LIMIT=100] -->
    <!-- Feedback shown during interactive parts of Assistant gesture tutorial when the gesture doesn't go diagonally enough. [CHAR LIMIT=100] -->
    <!-- Feedback shown during interactive parts of Assistant gesture tutorial when the gesture doesn't go far enough. [CHAR LIMIT=100] -->
    <!-- Title shown in sandbox mode part of gesture tutorial. [CHAR LIMIT=30] -->
    <!-- Subtitle shown in sandbox mode part of gesture tutorial. [CHAR LIMIT=60] -->
    <!-- Feedback shown in sandbox mode when the back gesture is successfully issued. [CHAR LIMIT=60] -->
    <!-- Feedback shown in sandbox mode when the assistant gesture is a successfully issued. [CHAR LIMIT=60] -->
    <!-- Feedback shown in sandbox mode when the home gesture is a successfully issued. [CHAR LIMIT=60] -->
    <!-- Feedback shown in sandbox mode when the overview gesture is a successfully issued. [CHAR LIMIT=60] -->
    <!-- Feedback shown in sandbox mode when the back gesture swipe is too far from the edge. [CHAR LIMIT=60] -->
    <!-- Title shown on the confirmation screen after successful gesture. [CHAR LIMIT=30] -->
    <string name="gesture_tutorial_confirm_title">Está tudo pronto</string>
    <!-- Button text shown on a button on the feedback popup to complete the gesture tutorial. [CHAR LIMIT=14] -->
    <string name="gesture_tutorial_action_button_label">Concluído</string>
    <!-- Button text shown on a button to go to Settings. [CHAR LIMIT=14] -->
    <string name="gesture_tutorial_action_button_label_settings">Definições</string>
    <!-- Feedback title to try again. [CHAR LIMIT=30] -->
    <string name="gesture_tutorial_try_again">Tente novamente</string>
    <!-- Feedback title for a successful gesture. [CHAR LIMIT=30] -->
    <string name="gesture_tutorial_nice">Boa!</string>
    <!-- Feedback subtext displaying the current step and the total number of steps for the tutorial. [CHAR LIMIT=30] -->
    <string name="gesture_tutorial_step">Tutorial <xliff:g id="CURRENT">%1$d</xliff:g>/<xliff:g id="TOTAL">%2$d</xliff:g></string>
    <!-- Title of "All Set" page [CHAR LIMIT=NONE] -->
    <string name="allset_title">Tudo pronto!</string>
    <!-- Hint string at the bottom of "All Set" page [CHAR LIMIT=NONE] -->
    <string name="allset_hint">Deslize rapidamente para cima para aceder ao ecrã principal</string>
    <!-- Description of "All Set" page on phones [CHAR LIMIT=NONE] -->
    <string name="allset_description">Já pode começar a utilizar o seu telemóvel</string>
    <!-- Description of "All Set" page on tablets [CHAR LIMIT=NONE] -->
    <string name="allset_description_tablet">Já pode começar a usar o seu tablet</string>
    <!-- String linking to navigation settings on "All Set" page [CHAR LIMIT=NONE] -->
    <string name="allset_navigation_settings"><annotation id="link">\"Definições de navegação do sistema\"</annotation></string>
    <!-- ******* Overview ******* -->
    <!-- Label for a button that causes the current overview app to be shared. [CHAR_LIMIT=40] -->
    <string name="action_share">Partilhar</string>
    <!-- Label for a button that causes a screen shot of the current app to be taken. [CHAR_LIMIT=40] -->
    <string name="action_screenshot">Fazer captura de ecrã</string>
    <!-- Label for a button that enters split screen selection mode. [CHAR_LIMIT=20] -->
    <string name="action_split">Dividir</string>
    <!-- Label for toast with instructions for split screen selection mode. [CHAR_LIMIT=50] -->
    <string name="toast_split_select_app">Toque noutra app para utilizar o ecrã dividido</string>
